{
  "cli": {
    "version": ">= 3.13.3"
  },
  "build": {
    "development": {
      "env": {
        "EXPO_PUBLIC_BASE_URL":"http://api.weatherapi.com/v1"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "env": {
        "EXPO_PUBLIC_BASE_URL":"http://api.weatherapi.com/v1"
      },
      "distribution": "internal",
      "android": {
        "developmentClient": true,
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": true
      }
    },
    "production": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BASE_URL":"http://api.weatherapi.com/v1"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }


    }
  },
  "submit": {
    "production": {}
  }
}
